%% BibTex-Datenbank-Datei
%% ======================
%%
%% Hier sind mal die meisten der möglichen BibTex-Einträge angegeben.
%% Wenn Du Deine BibTex-Datenbank zusammenstellst, verwende bitte 
%% Einträge dieser Form.
%%
%% Zwischen die geschw. Klammer auf und das Komma kommt das Label, 
%% das mit \cite{} angesprochen wird. Die Felder, die mit "OPT"
%% anfangen, sind optional. Willst Du sie verwenden, muss das OPT 
%% gelöscht werden. Bei einigen Einträgen gibt es alternative
%% Angaben (z.B. ALTauthor vs. ALTeditor). Da sucht man sich dann
%% einen raus und entfernt das "ALT".
%%
%% Als Label sind Kombinationen aus Autor und Jahr empfehlenswert,
%% z.B. Einstein1905b. Bitte keine Umlaute und Sonderzeichen im 
%% Label, das gibt nur Probleme.
%%
%% Autoren sind mit "and" zu trennen, Vornamen zuerst, z.B.
%%   author = {A. Einstein and M. Curie and I. Newton and others}
%% "and others" steht für weitere Autoren, wird zumeist in "et. al."
%% umgesetzt. Es gibt noch ein paar Spezialregeln, die aber meist
%% nicht interessieren.
%%
%% Für weitere Infos, siehe:
%% http://www.nwalsh.com/tex/texhelp/BibTeX.html

%% Die wichtigsten Typen im Überblick
%% article		Ein Artikel aus einer wissenschaftlichen Zeitschrift. Notwendige Felder: author, title (Titel des Beitrags), journal (Name der 		%%			Zeitschrift) year. Wünschenswerte Felder: year, pages
%% book			Ein Buch mit einem Herausgeber. Notwendige Felder: author oder editor, title, publisher, year.
%% inbook		Eine bestimmte Stelle in einem Buch, z.B. ein Kapitel oder eine Seite. Notwendige Felder: author oder editor, title, publisher, year.
%%			Falls ein Kapitel referenziert wird zusätzlich chapter (Titel des Kapitels), falls eine Seite referenziert wird page.
%% incollection		Ein Kapitel eines Buches, das einen Herausgeber und für die einzelnen Kapitel unterschiedliche Autoren hat.
%%			Notwendige Felder: author (des Kapitels), title (des Kapitels), booktitle (Titel des Buches), year, publisher, editor.
%%			Wünschenswerte Felder: pages.
%% inproceedings	Ein Beitrag (paper) in einem Konferenzband. Notwendige Felder: author, title (des Beitrags), booktitle (des Konferenzbandes), year.
%%			Wünschenswerte Felder: pages, address (Ort, Land), month.
%% mastersthesis	Eine Master- Diplom-, Bachelor- oder Projektarbeit. Notwendige Felder: author, title, school, year, type (Diploma Thesis / Master Thesis 			/..., unpublished/published). Wünschenswerte Felder: month.
%% phdthesis		Wie oben jedoch eine Dissertation.


%% Ein Beispiel (für einen Artikel in einer Zeitschrift):
@Article{Albiez03,
author = {J. Albiez and T. Luksch and K. Berns and R. Dillmann},
title = {An Activation-Based Behavior Control Architecture for Walking Machines},
journal = {The International Journal on Robotics Research, Sage Publications},
year = {2003},
volume = {22},
pages = {203--211}
}

%% Noch ein Beispiel (für ein Paper):
@inproceedings{Berns06,
  author = {K. Berns and J. Hirth},
  title = {Control of facial expressions of the humanoid robot head ROMAN},
  booktitle = {IEEE/RSJ International Conference on Intelligent Robots and Systems (IROS)},
  year = {2006},
  pages = {3119--3124},
  address = {Beijing, China},
  month = {October 9--15},
}

%% Noch ein Beispiel (ein Kapitel in einem Buch, in dem verschiedene Autoren verschiedene Kapitel geschrieben haben):
@InCollection{Ilg99,
  author = {W. Ilg and T. Muehlfriedel and K. Berns and  R. Dillmann},
  title = {Hybrid learning concepts for a biological inspired control of periodic movements  for  walking machines},
  booktitle = {Soft Computing in Mechatronics},
  year = {1999},
  pages = {19--38},
  publisher = {Springer Verlag},
  editor = {K.Hirota and T. Fukuda},
  month = {August},
}

%% Noch ein Beispiel (die Proceedings einer Konferenz):
@Proceedings{Berns07,
  title = {Autonome Mobile Systeme 2007},
  publisher = {Springer},
  year = {2007},
  editor = {K. Berns and T. Luksch},
  series = {Informatik aktuell},
  address = {Kaiserslautern},
  month = {October 18--19},
}

%% Noch ein Beispiel (ein Buch):
@Book{Arkin98,
  author =	 {R. Arkin},
  title =	 {Behavior-Based Robotics},
  year =	 {1998},
  publisher =	 {MIT Press}
}

%% Noch ein Beispiel (ein Kapitel in einem Buch - das Kapitel ist von dem Buch-Autor):
@InBook{Mendel70,
  author = {J.M. Mendel and R.W. McLaren},
  title = {Adaptive Learning and Pattern Recognition Systems: Theory and Applications},
  chapter = {Reinforcement learning control and pattern recognition systems},
  publisher = {Academic Press, New York},
  year = {1970},
  pages = {287--318}
}

%% Noch ein Beispiel (eine Doktorarbeit):
@PhdThesis{Breazeal00,
  author = {C.L. Breazeal},
  title = {Sociable Machines: Expressive Social Exchange Between Humans and Robots},
  school = {Massachusetts Institute Of Technology},
  year = {2000},
  month = {May}
}

%% Noch ein Beispiel (eine Diplomarbeit; Projektarbeiten werden genauso eingeragen nur mit 'Project' statt 'Diploma'):
@MastersThesis{Luksch02,
  author = {T. Luksch},
  title = {Verhaltensbasierte freie Gangart f\"{u}r eine vierbeinige Laufmaschine},
  school = {Universit\"{a}t Karlsruhe, Forschungszentrum Informatik (FZI) Karlsruhe},
  year = {2002},
  type = {Diploma Thesis, unpublished},
  month = {August}
}

%% Noch ein Beispiel (ein technischer Bericht):
@TechReport{Schaefer03,
  author = {J. Brandt and H. Sch\"{a}fer},
  title = {SmoothBrake --- A Prototyping Case Study},
  institution = {Robotics Research Lab - Technische Universit\"{a}t Kaiserslautern},
  year = {2003},
  type = {Technical Report, unpublished},
}

%% Beispiel für Verweis auf URL
@Misc{WWWBibTex,
  key =		 {BibTex},
  title =	 {Help on BibTex},
  howpublished = {\url{http://www.nwalsh.com/tex/texhelp/BibTeX.html}},
  year =	 {2003}
}


